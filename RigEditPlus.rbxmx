rotHandles.MouseDrag:connect(function(axis,relAngle,deltaRadius)
	--0print(axis,relAngle,deltaRadius)

	if globalAngleStep ~= 0 then
		local radStep = math.rad(globalAngleStep)
		relAngle = math.floor(relAngle/radStep)*radStep
	end
	--if distance == lastDistance then return end

	local refJoint
	for i,v in pairs(jointAdornments) do
		if v == rotHandles.Adornee.Parent then
			refJoint = i
		end
	end

	if not refJoint then return end	

	local axRot = Vector3.FromAxis(axis)*relAngle
	local rotBy = CFrame.Angles(axRot.x,axRot.y,axRot.z)

	for joint,config in pairs(originalJointConfigs) do
		if joint.Part0 and joint.Part1 then
			joint.C1 = config.C1*rotBy
			joint.C0 = config.C0*rotBy
		end
	end	
end)

