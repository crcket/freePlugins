toolButtons.CreateJoint.MouseButton1Down:connect(function()
	local sel = selection:Get()
	if #sel == 0 then
		warn("[RigEdit] Please select parts to join.")
		return
	end
	for i,v in pairs(sel) do
		if not v:IsA("BasePart") then
			warn("[RigEdit] Selection must contain only parts, found '"..v.ClassName.."' ("..v:GetFullName()..").")
			return
		end
	end

	local root = sel[1]
	table.remove(sel,1)

	local joints = {}

	changeHistory:SetWaypoint("CreateJointBefore")

	for i,v in pairs(sel) do
		v.Anchored = false
