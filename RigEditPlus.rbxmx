function getSelModel(sel)
	if #sel ~= 1 then
		warn("[RigEdit] Please select only one model or tool to rig.")
		return
	end

	local allowedTypes = {
		Model = true,
		Folder = true,
		Tool = true,
		Accessory = true,
	}

	local sm
	if not allowedTypes[sel[1].ClassName] then
		local function findAncestor(obj)
			local par = obj.Parent
			if not par or par:IsA("WorldRoot") or par == game then return false end
			if allowedTypes[par.ClassName] then return par end
			
			return findAncestor(par)
		end
		
		sm = findAncestor(sel[1])

		if not sm then		
			warn("[RigEdit] Please select a model or tool to rig.")
			return
		end
	end
	
	return sm or sel[1]
end

function refreshSelection()
	local sel = selection:Get()

	--

	if #sel == 1 then
		if selectUI.Enabled then
			warn("checking selection")
			selModel = getSelModel(sel)

			if selModel then
				warn("found one!")
				selectUI.Enabled = false
				editModel(selModel)
				return
			end
		end
	end

	selectionHash = {}
	selectingJoint = false
	for i,v in pairs(sel) do
		selectionHash[v] = i
		if not selectingJoint and (v:IsA("Weld") or v:IsA("Motor6D")) then selectingJoint = true end
	end

	for i,v in pairs(selectionBillboards) do
		local found = selectionHash[i]
		if found then
			if selModel then
				i.LocalTransparencyModifier = 0
			end
			v.TextLabel.Text = found-1
			if found == 1 then
				v.TextLabel.TextColor3 = Color3.new(1,0.95,0.65)
			else
				v.TextLabel.TextColor3 = Color3.new(1,1,1)
			end
		else
			v:Destroy()
			selectionBillboards[i] = nil

			if selModel and i:IsDescendantOf(selModel) then
				i.LocalTransparencyModifier = 0.5
			end
		end
	end

	for joint,adorn in pairs(jointAdornments) do
		if not adorn.Parent then jointAdornments[adorn] = nil end
		local hueVal = Color3.toHSV(adorn.Color3)
		local found = selectionHash[joint]
		if found then
			adorn.Color3 = Color3.fromHSV(hueVal,0.3,1)
			if adorn:FindFirstChild("Hover") then
				adorn.Hover.Visible = canView(joint)
			end
		else
			adorn.Color3 = Color3.fromHSV(hueVal,0.9,0.9)
			if jointHoveringOver ~= joint and adorn:FindFirstChild("Hover") then
				adorn.Hover.Visible = false
			end
		end
	end

	local selectingJoints = false
	if selModel then
		local selectingParts = false
		for i,v in pairs(sel) do
			if v:IsA("BasePart") then
				selectingParts = true
				if not selectionBillboards[v] then
					local bb = Instance.new("BillboardGui")
					bb.Adornee = v
					bb.Size = UDim2.new(0,40,0,40)
					bb.AlwaysOnTop = true
					local tl = Instance.new("TextLabel")
					tl.Size = UDim2.new(1,0,1,0)
					tl.Text = (#sel)-1
					if #sel == 1 then
						tl.TextColor3 = Color3.new(1,0.95,0.65)
					else
						tl.TextColor3 = Color3.new(1,1,1)
					end
					tl.TextSize = 18
					tl.BackgroundTransparency = 1

					tl.TextStrokeTransparency = 0.5
					tl.TextStrokeColor3 = Color3.new()
					tl.Font = Enum.Font.SourceSansBold

					tl.Parent = bb
					bb.Parent = GUI

					selectionBillboards[v] = bb
				end
			elseif canEdit(v) then
				selectingJoints = true
				-- create rotate and stuff
				local adorn = jointAdornments[v]
				if adorn then
					if handlesMode == MOVE_JOINT then
						moveHandles.Adornee = adorn:WaitForChild("RefPart",1)
					else
						rotHandles.Adornee = adorn:WaitForChild("RefPart",1)
						rotAdorn.Adornee = rotHandles.Adornee
						rotAdorn2.Adornee = rotHandles.Adornee
					end
				end
			end
		end
		if selectingParts then
			for joint,adorn in pairs(jointAdornments) do
				adorn.Visible = false
				for i,v in pairs(adorn:children()) do
					if v.Name == "Cap" or v.Name == "Pointer" then
						v.Visible = false
					end
				end
			end
		else
			for joint,adorn in pairs(jointAdornments) do
				adorn.Visible = canView(joint)
				for i,v in pairs(adorn:children()) do
					if v.Name == "Cap" or v.Name == "Pointer" then
						v.Visible = adorn.Visible
					end
				end
			end
		end
	end

	if not selectingJoints then
		moveHandles.Adornee = nil
		rotHandles.Adornee = nil
		rotAdorn.Adornee = nil
		rotAdorn2.Adornee = nil
	end
end

selection.SelectionChanged:connect(refreshSelection)

UIS.InputBegan:connect(function(inp,gameProc)
