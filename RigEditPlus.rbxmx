local function canView(joint)
	local adornType = (joint:IsA("Weld") and "Weld") or (joint:IsA("Motor6D") and "Joint")
	if not adornType then return false end
	return not not accessSettings[adornType.."View"]
end

local function canEdit(joint)
	local adornType = (joint:IsA("Weld") and "Weld") or (joint:IsA("Motor6D") and "Joint")
	if not adornType then return false end
	return not not accessSettings[adornType.."Edit"]
end

local lastStepText = stepBox.Text
stepBox.Changed:connect(function()
	local txt = stepBox.Text
	if txt == "." then
		lastStepText = txt
		return
	end

	if tonumber(txt) then
		globalStep = tonumber(txt)
		plugin:SetSetting("Step",globalStep)
		settingsChanged:Fire()
		lastStepText = txt
	elseif txt ~= "" then
		stepBox.Text = lastStepText
	end
end)

stepBox.FocusLost:Connect(function()
	if not tonumber(stepBox.Text) then
		stepBox.Text = lastStepText
	end
end)

