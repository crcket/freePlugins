local lastAngleStepText = angleStepBox.Text
angleStepBox.Changed:Connect(function()
	local txt = angleStepBox.Text
	txt = txt:gsub("°","")

	if txt == "." then
		lastAngleStepText = txt
		return
	end

	if tonumber(txt) then
		globalAngleStep = tonumber(txt)
		plugin:SetSetting("AngleStep",globalAngleStep)
		settingsChanged:Fire()
		lastAngleStepText = txt
	elseif txt ~= "" then
		angleStepBox.Text = lastAngleStepText
	end
end)

angleStepBox.FocusLost:Connect(function()
	local txt = angleStepBox.Text
	txt = txt:gsub("°","")
	
	if not tonumber(txt) then
		angleStepBox.Text = lastAngleStepText
	end
	
	if not angleStepBox.Text:match("°") then
		angleStepBox.Text ..= "°"
	end
end)

--

local lastScaleText = scaleBox.Text
scaleBox.Changed:Connect(function()
	local txt = scaleBox.Text
	txt = txt:gsub("x","")

	if txt == "." then
		lastScaleText = txt
		return
	end

	if tonumber(txt) then
		globalScale = tonumber(txt)

		if globalScale < 0.1 then
			globalScale = 0.1
		end

		plugin:SetSetting("Scale",globalScale)
		settingsChanged:Fire()
		
		partHoverBox.LineThickness = 0.01*globalScale
		
		lastScaleText = txt
	elseif txt ~= "" then
		scaleBox.Text = lastScaleText
	end
end)

scaleBox.FocusLost:Connect(function()
	local txt = scaleBox.Text
	txt = txt:gsub("x","")
	if not tonumber(txt) then
		scaleBox.Text = lastScaleText
	end
	
	if not scaleBox.Text:match("x") then
		scaleBox.Text ..= "x"
	end
end)

