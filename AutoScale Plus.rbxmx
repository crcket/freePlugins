					<ProtectedString name="Source"><![CDATA[local MenuHandler = {}
MenuHandler.__index = MenuHandler
local StarterGui = game:GetService("StarterGui")
local MenuBools = script.Parent:WaitForChild("MenuBools")
repeat wait() until #MenuBools:GetChildren() >= 7
local StudioLoadedModule = require(script.Parent:WaitForChild("StudioLoaded"))
repeat wait() until StudioLoadedModule.IsStudioLoaded()
local plugin = nil

--Functions
function MenuHandler.SyncSettingsBools(pluginvar)
	plugin = pluginvar
	for i,v in pairs(MenuBools:GetChildren()) do
		if v:IsA("BoolValue") then
			v.Value = pluginvar:GetSetting(v.Name)
		end
	end
end

local function PropertyExists(obj, prop)
	return ({pcall(function()if(typeof(obj[prop])=="Instance")then error()end end)})[1]
end

--Execution
StarterGui.DescendantAdded:Connect(function(descendant)
	if descendant:isA("GuiObject") and not descendant:IsA("ScreenGui") and not descendant:IsA("Folder") and not descendant:IsA("StarterGui")  then
		
		--DefaultAnchorPointIsCentre
		if PropertyExists(descendant, "AnchorPoint") and plugin:GetSetting("DefaultAnchorPointIsCentre") == true then
			descendant.AnchorPoint = Vector2.new(.5, .5)
			
			if plugin:GetSetting("DefaultPosIsOffset") == true then	
				if descendant.Position == UDim2.new(0,1,0,1) or descendant.Position == UDim2.new(0,0,0,0)then
					descendant.Position = UDim2.new(0, 50,0, 50)
				end
			else
				if descendant.Position == UDim2.new(0,0,0,0) then
					descendant.Position = UDim2.new(0.045,0,0.112,0)
				end	
			end
			
		end
		
		--DefaultBackgroundTransparencyIs1
		if PropertyExists(descendant, "BackgroundTransparency") and plugin:GetSetting("DefaultBackgroundTransparencyIs1") == true then
			if not descendant:IsA("Frame") and not descendant:IsA("ScrollingFrame") then
				descendant.BackgroundTransparency = 1
			end
		end
		
		--DefaultBorderSizePixelIs0
		if PropertyExists(descendant, "BorderSizePixel") and plugin:GetSetting("DefaultBorderSizePixelIs0") == true then
			descendant.BorderSizePixel = 0
		end	
			
		--DefaultTextScaledIsTrue
		if PropertyExists(descendant, "TextScaled") and plugin:GetSetting("DefaultTextScaledIsTrue") == true then
			descendant.TextScaled = true
		end	
		
		--DefaultPosIsOffset
		if plugin:GetSetting("DefaultPosIsOffset") == true then
			descendant.Position = UDim2.new(0, 1, 0, 1)
		end		
		
		--DefaultSizeIsScale
		if plugin:GetSetting("DefaultSizeIsScale") == true then
			game:GetService( 'RunService' ).RenderStepped:Wait()
			local Viewport_Size
			
			if descendant.Parent:IsA("GuiObject") then
				Viewport_Size = descendant.Parent.AbsoluteSize
			elseif descendant:FindFirstAncestorWhichIsA("GuiObject") then
				Viewport_Size = descendant:FindFirstAncestorWhichIsA("GuiObject").AbsoluteSize
			else
				Viewport_Size = workspace.CurrentCamera.ViewportSize
			end
			
			local LB_Size = descendant.AbsoluteSize
			descendant.Size = UDim2.new(LB_Size.X/Viewport_Size.X,0,LB_Size.Y/Viewport_Size.Y, 0)			
		end
		
		--DefaultBackgroundTransparencyIs1
		if PropertyExists(descendant, "ScaleType") and plugin:GetSetting("DefaultImageScaleTypeisFit") == true then
			descendant.ScaleType = Enum.ScaleType.Fit
		end
		
	end
end)


return MenuHandler
]]></ProtectedString>
