					<ProtectedString name="Source"><![CDATA[local StudioServiceLoaded = {}
StudioServiceLoaded.__index = StudioServiceLoaded

--List
StudioLoaded = false
local ServicesAvail = {"Workspace","Lighting","ReplicatedFirst","ReplicatedStorage","ServerScriptService","ServerStorage","StarterGui","StarterPack","Teams"}

for i,v in pairs(ServicesAvail) do
	local GotService = game:GetService(v)
	
	GotService.DescendantAdded:Connect(function()
		StudioLoaded = false
		wait(.5)
		if StudioLoaded == false then
			StudioLoaded = true
		end
	end)
	
	wait(.5)
	StudioLoaded = true
	
end
--Functions
function StudioServiceLoaded.IsStudioLoaded()
	return StudioLoaded
end


return StudioServiceLoaded
]]></ProtectedString>
