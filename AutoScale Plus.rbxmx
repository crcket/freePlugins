					<ProtectedString name="Source"><![CDATA[--SmartScaleLibrary V1.8
local SmartScaleLibrary = {}
SmartScaleLibrary.__index = SmartScaleLibrary

local AutoScaleLibrary = require(script.Parent:WaitForChild("AutoScaleLibrary"))
local ChangeHistoryService = game:GetService("ChangeHistoryService")


--Functions
local function PropertyExists(obj, prop)
	return ({pcall(function()if(typeof(obj[prop])=="Instance")then error()end end)})[1]
end

local function GetUiScreenPercentage(gui)
	if	PropertyExists(gui, "AbsoluteSize") then
		local screenGui = Instance.new('ScreenGui')
		screenGui.Parent = game:GetService("StarterGui")
		local screenGuiABS = screenGui.AbsoluteSize
		screenGui:Destroy()
		return math.ceil(((gui.AbsoluteSize.X + gui.AbsoluteSize.Y) / (screenGuiABS.X + screenGuiABS.Y)) * 100)
	end
end


--Smart Scale
function SmartScaleLibrary.SmartScale()
	for _, v in ipairs(game.Selection:Get()) do
		--ScreenGui
		if v:isA("ScreenGui") then
			v.IgnoreGuiInset = true
			for i,child in ipairs(v:GetChildren()) do
				if child:IsA("GuiBase2d") and not child:IsA("ScreenGui") then
					AutoScaleLibrary.AddConstraint(child)
				end
			end

			for o,descendant in ipairs(v:GetDescendants()) do
				if descendant:IsA("GuiBase2d") then
					if PropertyExists(descendant, "Size") and PropertyExists(descendant, "Position") then
						--Size
						local Viewport_Size

						if descendant.Parent:IsA("GuiObject") then
							Viewport_Size = descendant.Parent.AbsoluteSize
						elseif descendant:FindFirstAncestorWhichIsA("GuiObject") then
							Viewport_Size = descendant:FindFirstAncestorWhichIsA("GuiObject").AbsoluteSize
						else
							Viewport_Size = workspace.CurrentCamera.ViewportSize
						end

						local LB_Size = descendant.AbsoluteSize
						descendant.Size = UDim2.new(LB_Size.X/Viewport_Size.X,0,LB_Size.Y/Viewport_Size.Y, 0)

						--Position
						if descendant.Parent:IsA("GuiObject") and PropertyExists(descendant.Parent, "AbsoluteSize") then
							local ScaleXPos = descendant.Position.X.Offset/descendant.Parent.AbsoluteSize.X + descendant.Position.X.Scale
							local ScaleYPos = descendant.Position.Y.Offset/descendant.Parent.AbsoluteSize.Y  + descendant.Position.Y.Scale
							descendant.Position = UDim2.new(ScaleXPos, 0, ScaleYPos, 0)
						end

						--Dynamic Text Scaling
						if PropertyExists(descendant, "TextScaled") and not descendant:FindFirstChildWhichIsA("UITextSizeConstraint") then
							if descendant.TextScaled == false then
								local tconstraint = Instance.new("UITextSizeConstraint")
								tconstraint.MaxTextSize = descendant.TextSize
								tconstraint.Parent = descendant
								descendant.TextScaled = true
							elseif descendant.TextScaled == true then
								local textconstraint = Instance.new("UITextSizeConstraint")
								textconstraint.MaxTextSize = descendant.TextBounds.Y
								textconstraint.Parent = descendant				
							end
						end


					end
				end
			end				
		end 

		--GuiBase2d
		if v:isA("GuiBase2d") and not v:IsA("ScrollingFrame") then
			--Unit Conversion
			AutoScaleLibrary.SetSize("Scale", v)
			AutoScaleLibrary.SetPos("Scale", v)

			--If child of ScreenGui
			if v.Parent:IsA("ScreenGui") then
				v.Parent.IgnoreGuiInset = true
				AutoScaleLibrary.AddConstraint(v)
			end	

			--Dynamic Text Scaling
			if PropertyExists(v, "TextScaled") and not v:FindFirstChildWhichIsA("UITextSizeConstraint") then
				if v.TextScaled == false then
					local tconstraint = Instance.new("UITextSizeConstraint")
					tconstraint.MaxTextSize = v.TextSize
					tconstraint.Parent = v
					v.TextScaled = true
				elseif v.TextScaled == true then
					local textconstraint = Instance.new("UITextSizeConstraint")
					textconstraint.MaxTextSize = v.TextBounds.Y
					textconstraint.Parent = v				
				end
			end
			--ScrollingFrame
		elseif v:IsA("ScrollingFrame") then
			AutoScaleLibrary.SetSize("Scale", v)
			AutoScaleLibrary.SetPos("Scale", v)

			for i,a in pairs(v:GetChildren()) do
				AutoScaleLibrary.SetSize("Scale", a)
				AutoScaleLibrary.SetPos("Scale", a)
			end
		end
	end
	ChangeHistoryService:SetWaypoint("Smart Scaled")
	AutoScaleLibrary.CustomPrint("SmartScale Completed", script.Parent:WaitForChild("CustomPrint"))	
end

return SmartScaleLibrary]]></ProtectedString>
